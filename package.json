{
  "name": "rc-credit-card",
  "private": false,
  "version": "0.0.2",
  "type": "module",
  "author": "haliscicek.com",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/rc-credit-card.es.js",
      "require": "./dist/rc-credit-card.umd.js"
    }
  },
  "main": "./dist/rc-credit-card.umd.js",
  "module": "./dist/rc-credit-card.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "preview": "vite preview",
    "ci": "pnpm run lint && pnpm run build",
    "release": "pnpm run lint && pnpm run build && changeset publish"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/styled-components": "^5.1.29",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "react": "18.2.0 || >=18.x",
    "react-dom": "18.2.0 || >=18.x",
    "styled-components": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.51.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.0.3",
    "react-hooks": "^1.0.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-linter": "^1.2.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "peerDependencies": {
    "react": "18.2.0 || >=18.x",
    "react-dom": "18.2.0 || >=18.x"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn  eslint",
      "yarn  format"
    ]
  }
}
